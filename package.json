{
  "name": "ocmui-team-dashboard",
  "version": "1.0.0",
  "description": "OCMUI Team Dashboard - JIRA/GitHub integration, team timezones, and productivity tools",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:api": "node server/index.js",
    "start": "yarn build && lsof -ti:3017 | xargs kill -9 2>/dev/null || true; node server/index.js & sleep 2 && open http://localhost:3017 && wait",
    "start:dev": "lsof -ti:3017 | xargs kill -9 2>/dev/null || true; concurrently --names \"API,React\" --prefix-colors \"blue,green\" \"nodemon --watch server server/index.js\" \"yarn dev\""
  },
  "dependencies": {
    "@atlaskit/adf-utils": "^19.23.1",
    "@atlaskit/editor-wikimarkup-transformer": "^11.18.2",
    "@tanstack/react-query": "^5.89.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.0",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "marked": "^4.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-is": "^19.2.4",
    "recharts": "^3.7.0",
    "vite": "^5.4.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "nodemon": "^3.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0"
  },
  "keywords": [
    "jira",
    "github",
    "dashboard",
    "productivity",
    "team-tools"
  ],
  "author": "",
  "license": "MIT"
}
